                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    soh.SalesOrderID,
    sod.SalesOrderDetailID,
    soh.OrderDate,
    sod.ProductID,
    sod.OrderQty,
    sod.UnitPrice,
    sod.UnitPriceDiscount,
    p.Name            AS ProductName,
    p.ProductNumber,
    pc.Name           AS ProductCategory,
    per.BusinessEntityID AS CustomerBEID,
    per.FirstName,
    per.LastName,
    sp.Name           AS Region
FROM Sales.SalesOrderHeader AS soh
JOIN Sales.SalesOrderDetail AS sod
    ON soh.SalesOrderID = sod.SalesOrderID
JOIN Production.Product AS p
    ON p.ProductID = sod.ProductID
LEFT JOIN Production.ProductSubcategory AS psc
    ON psc.ProductSubcategoryID = p.ProductSubcategoryID
LEFT JOIN Production.ProductCategory AS pc
    ON pc.ProductCategoryID = psc.ProductCategoryID
JOIN Sales.Customer AS c
    ON c.CustomerID = soh.CustomerID
JOIN Person.Person AS per
    ON per.BusinessEntityID = c.PersonID   -- for Individual customers
LEFT JOIN Person.Address AS a
    ON a.AddressID = soh.ShipToAddressID
LEFT JOIN Person.StateProvince AS sp
    ON sp.StateProvinceID = a.StateProvinceID
WHERE soh.Status = 5  -- shipped
  AND soh.OrderDate  BETWEEN '2011-07-01' AND '2011-08-31'; -- daily slice</property>
